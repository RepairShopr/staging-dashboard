<p id="notice"><%= notice %></p>


<div class="row">
  <% @servers.each do |server| %>
      <div class="col-sm-4 col-xs-6" style="padding-bottom: 20px;">
        <div class="bs-component">
          <div class="card">
            <h3 class="card-info card-header"></h3>
            <h3 class="card-header"><%= server.name %></h3>
            <div class="card-block">
              <h5 class="card-title">Branch: <%= server.deploys.last.try(:git_branch) %></h5>
              <h6 class="card-subtitle text-muted"><%= server.status %></h6>
            </div>
            <img style="height: 200px; width: 100%; display: block;" src="<%= server.logo_url %>" alt="Card image">
            <div class="card-block">
              <p class="card-text"><%= server.reserved_for %></p>
              <p class="card-text">last commit msg here too</p>
              <a href="#" class="card-link">Card link</a>
              <a href="#" class="card-link">Another link</a>
            </div>
            <div class="card-footer text-muted text-xs-center">
              2 days ago
            </div>
          </div>
        </div>
      </div>
  <% end %>
</div>

<br>

